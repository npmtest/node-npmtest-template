{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "description": "#### basic test coverage for  [template (v0.17.5)](https://github.com/jonschlinkert/template)  [![npm package](https://img.shields.io/npm/v/npmtest-template.svg?style=flat-square)](https://www.npmjs.org/package/npmtest-template) [![travis-ci.org build-status](https://api.travis-ci.org/npmtest/node-npmtest-template.svg)](https://travis-ci.org/npmtest/node-npmtest-template)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "template": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmtest/node-npmtest-template",
    "keywords": [
        "coverage",
        "test",
        "template"
    ],
    "license": "MIT",
    "main": "lib.npmtest_template.js",
    "name": "npmtest-template",
    "nameAlias": "npmtest_template",
    "nameOriginal": "npmtest-template",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmtest/node-npmtest-template.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "npm install \"kaizhu256/node-utility2#alpha\" && utility2 shDeployHeroku",
        "postinstall": "[ ! -f npm_scripts.sh ] || ./npm_scripts.sh postinstall",
        "start": "PORT=${PORT:-8080} utility2 start test.js",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "2017.4.25"
}
